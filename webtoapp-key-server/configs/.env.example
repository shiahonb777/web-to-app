# .env 环境变量示例
# 复制此文件为 .env 并填入实际值

# 服务器配置
SERVER_PORT=8080
ENV=development

# 数据库配置
DB_TYPE=sqlite
DB_PATH=./data/keyserver.db

# JWT 配置
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRE_HOURS=24

# API 配置
API_KEY=your-api-key-change-in-production
SIGNATURE_SECRET=signature-secret-key-change-in-production
TIMESTAMP_TOLERANCE=300
